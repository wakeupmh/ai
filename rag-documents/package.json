{
  "name": "example-project",
  "version": "0.1.0",
  "bin": {
    "startup": "bin/startup.js"
  },
  "scripts": {
    "co:login": "sh ./login.sh",
    "build": "tsc",
    "debug": "export $(cat local.env | xargs) && export $(cat aws.env | xargs) && npm run test:local src/functions/bedrock/bedrock.local.ts",
    "watch": "tsc -w",
    "test:local": "npm run tl",
    "test": "jest --passWithNoTests --silent --noStackTrace --runInBand",
    "test:coverage": "jest --coverage --passWithNoTests",
    "lint": "eslint './{src,infra}/**/*.{js,ts,tsx}' --quiet",
    "lint:staged-files": "eslint --quiet --fix",
    "cdk": "cdk",
    "cdk:build": "npm run cdk -- synth",
    "cdk:diff": "npm run cdk -- diff",
    "cdk:deploy": "npm run cdk -- deploy --all --ci",
    "deploy:cdk": "npx cdk deploy --all --ci",
    "deploy:cdk-all": "npx cdk deploy --all --ci --require-approval never",
    "tl": "AWS_XRAY_CONTEXT_MISSING=IGNORE_ERROR ts-node -r dotenv/config --transpileOnly"
  },
  "engines": {
    "node": "18.x"
  },
  "devDependencies": {
    "@aws-lambda-powertools/commons": "^1.12.1",
    "@aws-lambda-powertools/logger": "^1.12.1",
    "@aws-lambda-powertools/metrics": "^1.12.1",
    "@aws-lambda-powertools/tracer": "^1.12.1",
    "@aws-sdk/client-s3": "^3.583.0",
    "@aws-sdk/client-sns": "^3.583.0",
    "@aws-sdk/client-sqs": "^3.583.0",
    "@aws-sdk/lib-dynamodb": "^3.585.0",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@side/jest-runtime": "^1.1.0",
    "@swc/core": "^1.3.85",
    "@swc/jest": "^0.2.29",
    "@types/aws-lambda": "^8.10.119",
    "@types/jest": "^29.5.4",
    "@types/node": "^18.17.11",
    "@types/source-map-support": "^0.5.10",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "@typescript-eslint/typescript-estree": "^6.4.1",
    "aws-cdk": "^2.93.0",
    "dotenv": "^16.3.1",
    "esbuild": "^0.19.12",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "lefthook": "^1.6.15",
    "ts-node": "^10.9.1",
    "typescript": "5.1"
  },
  "dependencies": {
    "@llamaindex/community": "^0.0.23",
    "llamaindex": "^0.5.7",
    "@aws-sdk/client-secrets-manager": "^3.556.0",
    "aws-cdk-lib": "^2.93.0",
    "aws-xray-sdk": "^3.5.3",
    "axios": "^1.6.5",
    "constructs": "^10.0.0",
    "knex": "^3.1.0",
    "knex-stringcase": "^1.4.6",
    "mysql2": "^3.9.5",
    "node-html-parser": "^6.1.6",
    "source-map-support": "^0.5.21",
    "zod": "^3.22.4"
  }
}
